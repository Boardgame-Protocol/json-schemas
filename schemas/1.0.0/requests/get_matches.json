{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://boardgame-protocol.github.io/json-schemas/schemas/1.0.0/requests/get_matches.json",
  "type": "object",
  "required": [
    "version",
    "type",
    "agent_id"
  ],
  "properties": {
    "version": {
      "type": "string",
      "pattern": "^\\d+\\.\\d+\\.\\d+$"
    },
    "type": {
      "type": "string",
      "const": "get_matches"
    },
    "agent_id": {
      "type": "string"
    }
  },
  "additionalProperties": false
}