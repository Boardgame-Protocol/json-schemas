{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://boardgame-protocol.github.io/json-schemas/schemas/1.0.0/responses/create_match.json",
  "title": "Create Match Response",
  "type": "object",
  "required": [
    "version",
    "type",
    "game",
    "match_id",
    "agent_id"
  ],
  "properties": {
    "version": {
      "type": "string",
      "pattern": "^\\d+\\.\\d+\\.\\d+$"
    },
    "type": {
      "type": "string",
      "const": "create_match_response"
    },
    "game": {
      "type": "string"
    },
    "match_id": {
      "type": "string"
    },
    "agent_id": {
      "type": "string"
    }
  },
  "additionalProperties": false
}