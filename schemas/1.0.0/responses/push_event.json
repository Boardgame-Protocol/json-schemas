{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://boardgame-protocol.github.io/json-schemas/schemas/1.0.0/responses/push_event.json",
  "type": "object",
  "required": [
    "version",
    "game",
    "match_id",
    "agent_id",
    "turn",
    "stage",
    "state",
    "active_agent_id",
    "agents",
    "started_at"
  ],
  "properties": {
    "version": {
      "type": "string",
      "pattern": "^\\d+\\.\\d+\\.\\d+$"
    },
    "game": {
      "type": "string"
    },
    "match_id": {
      "type": "string"
    },
    "agent_id": {
      "type": "string"
    },
    "event": {
      "type": "object",
      "required": [
        "version",
        "data"
      ],
      "properties": {
        "version": {
          "type": "string",
          "pattern": "^\\d+\\.\\d+\\.\\d+$"
        },
        "data": {
          "type": "object"
        }
      }
    }
  }
}